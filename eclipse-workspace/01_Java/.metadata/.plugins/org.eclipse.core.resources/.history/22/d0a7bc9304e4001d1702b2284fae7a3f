
public class MainClass {

	public static void main(String[] args) {
		// 산술연산자
        // 사칙연산과 나머지를 구한다.
        // 더하기
        int a1 = 10 + 3;
        // 빼기
        int a2 = 10 - 3;
        // 곱하기
        int a3 = 10 * 3;
        // 나누기
        int a4 = 10 / 3;
        // 나머지
        int a5 = 10 % 3;

        System.out.printf("a1 : %d\n", a1);
        System.out.printf("a2 : %d\n", a2);
        System.out.printf("a3 : %d\n", a3);
        System.out.printf("a4 : %d\n", a4);
        System.out.printf("a5 : %d\n", a5);

	}

}
