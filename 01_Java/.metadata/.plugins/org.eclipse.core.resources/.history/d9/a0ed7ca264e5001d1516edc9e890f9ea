import java.util.Scanner;

public class Ex7 {

	public static void main(String[] args) {
		// 주민등록 번호를 입력받아 올바른 주민 등록 번호인지 확인한다.
        // (2 x 1자리) + (3 x 2자리) + (4 x 3자리) + (5 x 4자리) 
        // + (6 x 5자리) + (7 x 6자리) + (8 x 7자리) + (9 x 8자리)
        // + (2 x 9자리) + (3 x 10자리) + (4 x 11자리) + (5 x 12자리) 
        // 의 합을 구한다.
        // 위의 합을 11로 나눈 나머지를 구한다.
        // 11에서 나머지를 뺀다.
        // 뺀 숫자를 10으로 나눈 나머지를 구한다. 이 값이 13번째 자리 숫자가 된다.
		
		Scanner sc = new Scanner(System.in);
		System.out.print("주민번호 입력 : ");
		String id = sc.next();
		int sum = 0;
		int[] number = {2, 3, 4, 5, 6, 7, 0, 8, 9, 2, 3, 4, 5};
		
		for(int i=0; i<id.length()-1; i++) {
			if(id.charAt(i) == '-') {
				continue;
			}
			sum += (int)id.charAt(i) * number[i];
		}
		int total = 11 - sum % 11;
		if (total >= 10) {
			total %= 10;
		}
		
		if(total == (int)id.charAt(id.length()-1)) {
			System.out.println("올바른 주민번호 입니다.");
		} else {
			System.out.println("올바르지 않은 주민번호 입니다.");
		}
	}

}
